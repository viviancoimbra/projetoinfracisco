!Entrar no modo Exec privilegiado
enable
   clock set 19:50:00 17 October 2025
   configure terminal
        hostname sw-04
        service password-encryption
        service timestamps log datetime msec
        logging buffered 4096
        no ip domain-lookup
        banner motd #Vivian Coimbra LGPD#
        enable secret 123@senac
        username senac secret 123@senac
        username tatuape password 123@senac
        username admin privilege 15 secret 123@senac
        no cdp  run
        no lldp run
        line console 0
            login local 
            password 123@senac
            logging synchronous 
            exec-timeout 5 30 
            exit
        !Configurando a line VTY
        line vty 0 4
            login local
            password 123@senac
            exec-timeout 5 30
            transport input ssh 
            logging synchronous
            exit
        !configurando o SVI
        ip default-gateway 172.16.100.254 
        interface vlan 1
            description Interface de SVI
            ip address 172.16.100.251 255.255.255.0
            no shutdown
            exit
        !configurando o SSH
         ip domain-name senac.br
        crypto key generate rsa general-keys modulus 1024
        ip ssh version 2
        ip ssh time-out 60
        ip ssh authentication-retries 2
        end 
    

    write


